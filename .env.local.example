# Development Environment Configuration
# Copy this file to .env.local and fill in your development Firebase credentials

# Firebase Configuration for Development
VITE_FIREBASE_API_KEY=your_development_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_dev_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_dev_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_dev_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_dev_messaging_sender_id
VITE_FIREBASE_APP_ID=1:your_dev_app_id:web:your_dev_web_config

# Development Admin Emails
VITE_ADMIN_EMAILS=dev@localhost.com,admin@dev.local

# Development Features
# Uncomment to enable specific development features
# VITE_DEBUG_MODE=true
# VITE_MOCK_API=false
